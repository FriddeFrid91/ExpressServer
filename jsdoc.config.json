{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
      "include": ["src", "test"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "destination": "doc/",
      "recurse": true,
      "verbose": true
    }
  }
  